-- =========================================
-- WAOW HUB - ADVANCED TELEPORT SYSTEM (4 TEAMS)
-- =========================================

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Teleport Konumları (Görsellerden Alınan Hassas Veriler)
local POSITIONS = {
    GreenSide = Vector3.new(0.1805742383003235, 175.29998779296875, 179.08871459960938),
    BlueSide = Vector3.new(0.3336418867111206, 175.29998779296875, 379.17535400390625),
    -- Görsel 1: Green Side GK
    GreenSideGK = Vector3.new(0.30000001192092896, 175.29998779296875, 59.500003814697266),
    -- Görsel 2: Blue Side GK
    BlueSideGK = Vector3.new(-0.7000000476837158, 175.29998779296875, 496.0000305175781)
}

-- Geçici ForceField (Kalkan) Ekler
local function addForceField(character)
    if character and character:FindFirstChild("HumanoidRootPart") then
        for _, v in pairs(character:GetChildren()) do
            if v:IsA("ForceField") then v:Destroy() end
        end
        local forceField = Instance.new("ForceField")
        forceField.Parent = character
        game:GetService("Debris"):AddItem(forceField, 1) -- 1 saniye kalkan
    end
end

-- Oyuncuyu Belirtilen Konuma Işınlar
local function teleportPlayer(position)
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local humanoidRootPart = LocalPlayer.Character.HumanoidRootPart
        humanoidRootPart.CFrame = CFrame.new(position)
        addForceField(LocalPlayer.Character)
    end
end

-- Takıma Göre Işınlanma Konumunu Belirler
local function getTeleportPosition()
    if LocalPlayer.Team then
        local teamName = LocalPlayer.Team.Name
        
        -- Önce GK takımlarını kontrol ediyoruz (Çakışmayı önlemek için)
        if teamName == "Green Side GK" then
            return POSITIONS.GreenSideGK
        elseif teamName == "Blue Side GK" then
            return POSITIONS.BlueSideGK
        -- Sonra normal takımları kontrol ediyoruz
        elseif teamName:lower():find("green") or teamName:lower():find("yeşil") then
            return POSITIONS.GreenSide
        elseif teamName:lower():find("blue") or teamName:lower():find("mavi") then
            return POSITIONS.BlueSide
        end
    end
    -- Takım bulunamazsa varsayılan
    return POSITIONS.GreenSide
end

-- Teleport İşlemini Çalıştırır
local function executeTeleport()
    if not LocalPlayer.Character then
        LocalPlayer.CharacterAdded:Wait()
    end
    local targetPosition = getTeleportPosition()
    
    -- Çift ışınlanma (Roblox'un konum hatasını önlemek için)
    teleportPlayer(targetPosition)
    task.wait(0.5)
    teleportPlayer(targetPosition)
end



