sections.ReactRight:Button({
    Name = "Main React", -- [cite: 2026-01-28]
    Callback = function()
        local lp = game.Players.LocalPlayer
        local ScorerRemote = workspace:WaitForChild("FE"):WaitForChild("Scorer"):WaitForChild("RemoteEvent")

        -- Kamera ve Tool çakışmasını önleyen güvenli hook [cite: 2026-01-28]
        local old; old = hookmetamethod(game, "__namecall", function(self, ...)
            local method = getnamecallmethod()
            local args = {...}

            if not checkcaller() and (method == "FireServer" or method == "InvokeServer") then
                -- SADECE Scorer (Dokunuş) anında tetiklenir
                if self == ScorerRemote then
                    task.spawn(function()
                        local ball = args[1] 

                        if ball and typeof(ball) == "Instance" then
                            -- 1. KeepP Bloğu (5 Tane) - Topu yakın tutar [cite: 2026-01-28]
                            for i = 1, 5 do
                                workspace.FE.Keep.KeepP:FireServer(ball)
                            end

                            -- 2. Attach Bloğu (5 Tane) - Mıknatıs etkisi [cite: 2026-01-28]
                            for i = 1, 5 do
                                workspace.FE.Attach.RemoteEvent:FireServer(ball)
                            end
                            
                            -- Dribble paketi tamamen kaldırıldı! [cite: 2026-01-28]
                            -- Artık Stop'a bastığında Stop, Pas'a bastığında Pas çalışacak. [cite: 2026-01-28]
                        end
                    end)
                end
            end
            return old(self, ...)
        end)
    end
})
sections.ReactRight:Button({
    Name = "React Killer", -- İstediğin isimle güncellendi [cite: 2026-01-28]
    Callback = function()
        local lp = game.Players.LocalPlayer
        local ScorerRemote = workspace:WaitForChild("FE"):WaitForChild("Scorer"):WaitForChild("RemoteEvent")

        -- Kamera hatasını (ZoomController) önlemek için temiz hook yöntemi
        local old; old = hookmetamethod(game, "__namecall", function(self, ...)
            local method = getnamecallmethod()
            local args = {...}

            if not checkcaller() and (method == "FireServer" or method == "InvokeServer") then
                -- Sadece şut/dokunuş anında (Scorer) devreye girer
                if self == ScorerRemote then
                    task.spawn(function()
                        local ball = args[1] 
                        
                        if ball and typeof(ball) == "Instance" then
                            -- Karar kıldığımız 35'lik tam kıvamında paket döngüsü [cite: 2026-01-28]
                            for i = 1, 35 do 
                                -- Kaleci imha paket dizilimi
                                workspace.FE.Keep.GK:FireServer(ball)
                                workspace.FE.GK.BGKSaves:FireServer(ball)
                                workspace.FE.GK.BGKP:FireServer(ball)
                                workspace.FE.GK.GGKP:FireServer(ball)
                                
                                -- Akıcılığı bozmayan mikrosaniyelik mola [cite: 2026-01-28]
                                if i % 7 == 0 then task.wait() end 
                            end
                        end
                    end)
                end
            end
            return old(self, ...)
        end)
    end
})
