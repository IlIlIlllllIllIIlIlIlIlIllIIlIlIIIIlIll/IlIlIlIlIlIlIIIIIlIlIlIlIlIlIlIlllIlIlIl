-- EN BAŞA BUNU KOYDUĞUNDAN EMİN OL
local player = game.Players.LocalPlayer
local lastTouch = 0
local touchCount = 0

----------------------------------------
-- FUNCTIONS
----------------------------------------
local function performInstantTurn()
    local char = player.Character
    if not char then return end

    -- Turuncu ikonlu Dribble kontrolü
    local tool = char:FindFirstChild("Dribble")
    
    if tool then 
        -- Sarı klasördeki TurnR
        local dataFolder = char:FindFirstChild("ToolDribble") or player.Backpack:FindFirstChild("ToolDribble")
        local gui = player:FindFirstChild("PlayerGui")
        
        if dataFolder and gui then
            local turnR = dataFolder:FindFirstChild("TurnR")
            if turnR then
                turnR.Value = not turnR.Value
                local animator = char:FindFirstChild("Humanoid"):FindFirstChild("Animator")
                local anims = gui:FindFirstChild("Animations")
                if animator and anims then
                    local animName = turnR.Value and "Turn" or "TurnL"
                    local animObj = anims:FindFirstChild(animName)
                    if animObj then
                        animator:LoadAnimation(animObj):Play()
                    end
                end
            end
        end
    end
end

----------------------------------------
-- AUTO SKILL TAB
----------------------------------------
local AutoSkillLeftSection = tabs.AutoSkill:Section({ Side = "Left" })
_G.touchThreshold = 0 

AutoSkillLeftSection:Slider({
    Name = "Dribble Z Touch Count",
    Default = 0,
    Minimum = 0,
    Maximum = 5,
    Precision = 0,
    Suffix = "",
    Tooltip = "0 = Off | Adjust touch count",
    Pointer = "AutoSkillTouchCount",
    Callback = function(value)
        _G.touchThreshold = value
    end
})

-- HATA VEREN 1128. SATIRIN DÜZELTİLMİŞ HALİ
-- Karakterin yüklenmesini bekleyerek 'nil' hatasını engelliyoruz
player.CharacterAdded:Connect(function(char)
    char:WaitForChild("HumanoidRootPart").Touched:Connect(function(hit)
        if _G.touchThreshold == 0 then return end
        if hit.Name == "TPS" or hit.Name == "PSoccerBall" then
            if tick() - lastTouch > 0.3 then
                touchCount = touchCount + 1
                lastTouch = tick()
                if touchCount >= _G.touchThreshold then
                    performInstantTurn()
                    touchCount = 0 
                end
            end
        end
    end)
end)

-- Mevcut karakter için de çalışması için:
if player.Character then
    player.Character:WaitForChild("HumanoidRootPart").Touched:Connect(function(hit)
        if _G.touchThreshold == 0 then return end
        if hit.Name == "TPS" or hit.Name == "PSoccerBall" then
            if tick() - lastTouch > 0.3 then
                touchCount = touchCount + 1
                lastTouch = tick()
                if touchCount >= _G.touchThreshold then
                    performInstantTurn()
                    touchCount = 0 
                end
            end
        end
    end)
end
